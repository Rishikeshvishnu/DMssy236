<owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/IOLite.owl#Grapheme">
  <rdfs:label xml:lang="en">Grapheme</rdfs:label>
  <rdfs:label xml:lang="it">Grafema</rdfs:label>
